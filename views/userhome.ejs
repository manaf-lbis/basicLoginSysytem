<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
</head>
<body>


    <!--================ nav bar=============== -->
    <nav class="navbar navbar-expand-lg bg-body-tertiary">
        <div class="container-fluid">
          <a class="navbar-brand" href="/">User Dashboard</a>
         
          <div class="collapse navbar-collapse" id="navbarScroll">
            <ul class="navbar-nav me-auto my-2 my-lg-0 navbar-nav-scroll" style="--bs-scroll-height: 100px;">
              
            </ul>
            <form method="post" action="/logout" class="d-flex" role="search">
              <button class="btn btn-outline-danger" type="submit">Logout</button>
            </form>
          </div>
        </div>
      </nav>
     <!--================ nav bar ends=============== -->

     <div class="card text-center">
        <div class="card-header">
         <h3>Hai <%= userData.name %>, Welcome Back</h3> 
        </div>
        <div class="card-body">
          <h5 class="card-title">Your Deatails are Here.</h5>
          <p class="card-text text-primary">You are login with Username :   <span class="text-danger"><%= userData.username %></span></p>
          <p class="card-text">Email ID :  <span class="text-danger"><%= userData.email %></span></p>
          <p class="card-text">Role :<span class="text-danger"><%= userData.role %></span></p>

          
        </div>
        <div class="card-footer text-body-secondary">
          Thank You....
        </div>
      </div>

     


    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
</body>
</html>